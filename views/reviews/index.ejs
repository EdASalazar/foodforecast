<%- include('../partials/header') %>
<table id="all-reviews">
    <thead>
        <tr>
            <th>Vendor</th>
            <th>Rating</th>
            <th>Price</th>
            <th>Author</th>
        </tr>
    </thead>
    <tbody>
        <% reviews.forEach(function(r) { %>
            <td><%= r.vendor %></td>
            <td><%= r.rating %></td>
            <td><%= r.price %></td>
            <td>USER</td>
       <% }); %>
    </tbody>
</table>


<%- include('../partials/footer') %>